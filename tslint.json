{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "semicolon": {
      "options": "never",
      "severity": "warn"
    },
    "quotemark": {
      "options": "single",
      "severity": "warn"
    },
    "no-empty-interface": false
  },
  "rulesDirectory": []
}
